% Parametres physiques :
L = 100
plants = 2000
animals = 600
tfin = 100

load = true
file to load = Saves/ecosystemSaveTest.out

save = true
save at t = 0     % regarder pour la convention du temps

write data = false
evolution = true
feeding = constant
feeding_rate = 0.02
mutation_rate = 0.1
write characteristics = bnnnn  % c (cell), a (animal), b (both), n (not)

shock at t = 700
shock parameter = 1.  %for now it's by what we multiply the feeding rate at shock

Animal zone = whole
Animal zone parameter 1 = 0
Animal zone parameter 2 = 0
Animal zone parameter 3 = 0
Animal zone parameter 4 = 50

Plant zone = whole
Plant zone parameter 1 = 0
Plant zone parameter 2 = 0
Plant zone parameter 3 = 100
Plant zone parameter 4 = 100

output = test

% good values L = 100, plants = 200, animals = 50
